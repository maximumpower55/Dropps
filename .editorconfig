root=true

[*]
charset = utf-8
end_of_line = lf
tab_width = 4
trim_trailing_whitespace = true
insert_final_newline = true

[*.java]
indent_style = tab

[*.json]
indent_style = space
indent_size = 2

[*.properties]
indent_style = space
indent_size = 2

[.editorconfig]
indent_style = space
indent_size = 4
